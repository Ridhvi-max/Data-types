name = "penguin"
age = 15
is_student=True
weight = 38.5
print("Name :", name)
print("Data type of name is", type(name))
print("Age :", age)
print("Data type Age is", type(age))
print("is_student :", is_student)
print("Data type of is_student is", type(is_student))
print("Weigth :", weight)
print("Data type of weigth is", type(weight))
